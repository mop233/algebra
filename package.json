{
  "name": "@mop233/algebra",
  "version": "1.1.3",
  "description": "A small, fast, easy-to-use library for primary mathematics.",
  "keywords": [
    "algebra",
    "number",
    "decimal",
    "fraction",
    "operation"
  ],
  "author": {
    "name": "Paul",
    "email": "37662788@qq.com",
    "url": "https://mopsite.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mop233/algebra.git"
  },
  "homepage": "https://mop233.github.io/algebra",
  "bugs": {
    "url": "https://github.com/mop233/algebra/issues"
  },
  "type": "module",
  "main": "./dist/algebra.js",
  "types": "./algebra.d.ts",
  "exports": {
    ".": {
      "types": "./algebra.d.ts",
      "import": "./dist/algebra.js",
      "require": "./dist/algebra.umd.js"
    }
  },
  "files": [
    "dist/",
    "LICENCE",
    "algebra.d.ts",
    "README.zh-CN.md"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "unpkg": "dist/algebra.umd.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "push": "git push && git subtree push --prefix docs/.vitepress/dist origin gh-pages"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "markdown-it-mathjax3": "^4.3.2",
    "rollup": "^4.21.0",
    "vitepress": "^1.6.3"
  }
}
